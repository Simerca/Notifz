{
  "name": "localnotification-react-native-sdk",
  "version": "1.0.9",
  "description": "React Native SDK for LocalNotification - Remote local notification management with user segmentation and analytics",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "expo-notifications": ">=0.20.0",
    "expo-localization": ">=14.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "expo-notifications": {
      "optional": false
    },
    "expo-localization": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/react": "^18.3.17",
    "expo-localization": "^15.0.0",
    "expo-notifications": "^0.29.13",
    "react": "^18.3.1",
    "react-native": "^0.76.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "keywords": [
    "react-native",
    "local-notification",
    "notifications",
    "expo",
    "push-notifications",
    "segmentation",
    "analytics",
    "retention"
  ],
  "author": "Aspect Music",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aspect-music/localnotification"
  },
  "homepage": "https://github.com/aspect-music/localnotification#readme",
  "bugs": {
    "url": "https://github.com/aspect-music/localnotification/issues"
  }
}
